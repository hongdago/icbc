<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE form-validation PUBLIC
     "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.4.0//EN"
     "http://commons.apache.org/dtds/validator_1_4_0.dtd">

<form-validation>
   <global>
      <validator name="required"
                 classname="com.cfm.bankinterface.validatorrule.DefaultValidateRule"
                 method="validateRequired"
                 methodParams="java.lang.Object,org.apache.commons.validator.Field"
                 msg="errors.required"/>
   </global>
   <formset>
      <form name="ICBC_QHBRequest">
         <field property="acctno"  depends="required">
            <arg key="acctno.display"/>
         </field>
         <field  property="beginDate" depends="required">
              <arg key="beginDate.display"/>
         </field>
         <field  property="endDate" depends="required">
              <arg key="endDate.display"/>
         </field>
      </form>
   </formset>
</form-validation>
