<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE form-validation PUBLIC
     "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.4.0//EN"
     "http://commons.apache.org/dtds/validator_1_4_0.dtd">

<form-validation>
   <formset>
      <form name="ICBC_QHB_Req">
         <field property="acctno"  depends="required">
            <arg key="acctno.display"/>
         </field>
         <field  property="beginDate" depends="date">
              <arg key="beginDate.display"/>
              <var>
              	<var-name>datePattern</var-name>
              	<var-value>yyyyMMdd</var-value>
              </var>
         </field>
         <field  property="endDate" depends="after">
              <arg key="endDate.display" position="0"/>
              <arg name="after" key="beginDate.display" position="1"/>
              <var>
              	<var-name>datePattern</var-name>
              	<var-value>yyyyMMdd</var-value>
              </var>
              <var>
              	<var-name>compareDate</var-name>
              	<var-value>beginDate</var-value>
              </var>
         </field>
      </form>
   </formset>
</form-validation>
